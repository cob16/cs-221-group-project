<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rpsrec_proto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Orange" >
        <activity
            android:name="info.cormacbrady.rpsrec.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.BlankRecord"
            android:label="@string/title_activity_blank_record" >
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.NewRecord"
            android:label="@string/title_activity_new_record"
            android:parentActivityName="info.cormacbrady.rpsrec.BlankRecord" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rpsrec_proto.BlankRecord" />
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.UserDataView"
            android:label="@string/title_activity_user_data_view" >
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.RecordView"
            android:label="@string/title_activity_record_view" >
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.TempRecordView"
            android:label="@string/title_activity_record_view" >
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.Fragment_test"
            android:label="@string/title_activity_fragment_test" >
        </activity>
        <activity
            android:name="info.cormacbrady.rpsrec.MainView"
            android:label="@string/title_activity_fragment_test" >
        </activity>
        <meta-data android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />
    </application>
<uses-permission android:name="android.permission.INTERNET" /> 
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>


</manifest>